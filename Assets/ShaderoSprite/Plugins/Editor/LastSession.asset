    \ : TEXCOORD0;\nfloat4 vertex   : SV_POSITION;\nfloat3 worldPos : TEXCOORD2;\nfloat4
    color    : COLOR;\n};\n\nsampler2D _MainTex;\nfloat _SpriteFade;\nfloat _GenerateLightning_PosX_1;\nfloat
